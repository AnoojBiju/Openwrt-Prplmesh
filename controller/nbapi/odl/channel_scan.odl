%config {
	define-behavior = { 
		existing-object = "update"
	};
}

%define {
	select Controller.Network.Device.Radio.ScanResult.OpClassScan.ChannelScan {

		%read-only uint32 Channel = 0 {
			on action validate call check_range [0, 255];
		}

		%read-only string TimeStamp = "0";

		%read-only uint32 Utilization = 0 {
			on action validate call check_range [0, 255];
		}

		%read-only uint32 Noise = 0 {
			on action validate call check_range [0, 220];
		}

		object NeighborBSS [] {
			counted with NumberOfNeighbors;
		}
	}
}
